#!/bin/sh /etc/rc.common 

START=99
USE_PROCD=1
PROG=/usr/sbin/iotm

start_service() {
	local enbale=`uci -q get iot.iotm.enable`
	if [ "enbale" == "0" ];then
		return
	fi
	procd_open_instance
	procd_set_param command "$PROG"
	procd_set_param respawn 0
	procd_close_instance
}

stop_service(){
	killall -9 iotm
}

