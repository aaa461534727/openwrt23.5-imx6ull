config modem 'main'
        option modem_tactics '0'
        option modem_num '1'
        option modem_tactics_usb '1'
        option wan_modem0 'wan_modem'
        option wan_modem1 'wan2_modem'
        option mcm_sms_type '0'
	option sim 'slot1'

config modem 'apn'
        option auth 'NONE'

config modem 'apnlist'
	option num '0'

config modem 'strategy'
	option prio '1'
	option route_mode '0'
	option detect_net '0'
	option port_list '53,53,53'
	option ip_list '114.114.114.114,208.67.222.222,8.8.8.8'
	option searchnetmode '0'

config modem 'smsInfo'
	option onlineEnable '0'
	option onlineText ''
	option offlineEnable '0'
	option offlineText ''
	option rebootEnable '0'
	option rebootText ''
	option phone0 ''
	option phone1 ''
	option phone2 ''
	option phone3 ''
	option phone4 ''
	option phone5 ''

config modem 'status'
